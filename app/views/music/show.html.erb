<div class="min-h-screen w-full bg-gray-600">
  <div class="max-w-5xl w-full mx-auto p-8 grid grid-cols-4 gap-8">
    <% @songs.each do |song| %>
      <div class="w-full" >
        <%= image_tag(song.image, class: "h-40 w-full object-cover rounded-lg") %>
        <p class="text-xl text-gray-100"> <%= song.title %> </p>
        <p class="text text-gray-200 font-bold"> <%= song.artist.stage_name %>
      </div>
    <% end %>
  </div>
</div>
