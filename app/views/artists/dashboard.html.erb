<div class="w-full min-h-screen flex flex-col items-center pt-20 px-10">
  <div class="max-w-5xl w-full">
    <h1 class="text-5xl font-bold text-red-900">Welcome to the Artist Dashboard</h1>
    <div class="flex gap-10 pt-10">
      <%= link_to "Post a song", new_song_path, class: "font-semibold p-2 rounded-lg bg-green-700 text-white hover:bg-gray-300 hover:text-black" %>
      <%= link_to "View your songs", songs_path, class: "font-semibold p-2 rounded-lg bg-blue-500 text-white hover:bg-gray-300 hover:text-black" %>
      <%= link_to "Edit your account", edit_artist_registration_path, class: "font-semibold p-2 rounded-lg bg-yellow-500 text-white hover:bg-gray-300 hover:text-black " %>
      <%= link_to "Sign out", destroy_artist_session_path, class: "font-semibold p-2 rounded-lg bg-red-500 text-white hover:bg-gray-300 hover:text-black", data: { turbo_method: :delete } %>
    </div>
  </div>

  <div data-controller="stripe-onboarding" data-stripe-onboarding-url-value="<%= onboarding_session_path %>"> 
  </div>
</div>

